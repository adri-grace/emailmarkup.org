---
let posts = await Astro.glob('../pages/en/reports/accessibility/*.(md|mdx)');
posts = posts
.filter(post => !post.file.endsWith('index.mdx'))
.sort((a, b) => new Date(b.frontmatter.publishDate).valueOf() - new Date(a.frontmatter.publishDate).valueOf());
---

<ul>
  {posts.map(post => (
    <li>
      <a href={post.url}>
        {post.frontmatter.title}
      </a>
    </li>
  ))}
</ul>